{
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "React.createElement",
        "pragmaFrag": "React.Fragment"
      }
    ],
    "./src/cli/babel-pexo-transform"
  ],
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ],
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "shippedProposals": true,
        "bugfixes": true
      }
    ]
  ]
}
