{
  "name": "@pexo/example",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "PEXO_EXPERIMENTAL=true pexo develop --server-entry ./server.tsx --client-entry ./client.tsx",
    "build": "PEXO_EXPERIMENTAL=true pexo build --server-entry ./server.tsx --client-entry ./client.tsx",
    "serve-build": "npm run build && node ./dist/server/server.js"
  },
  "targets": {
    "server": {
      "context": "node",
      "includeNodeModules": false,
      "sourceMap": true
    },
    "client": {
      "context": "browser",
      "sourceMap": true
    }
  },
  "author": "Jannick Garthen <jannick.garthen@gmail.com>",
  "license": "MIT",
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "engines": {
    "parcel": "^2.0.0-nightly.196",
    "node": ">=12.x"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@pexo/babel-transform": "0.0.0-alpha.2",
    "@pexo/cli": "0.0.0-alpha.1.3",
    "@pexo/parcel-reporter-manifest": "0.0.0-alpha.2",
    "@pexo/parcel-runtime-dynamic-imports": "0.0.0-alpha.2",
    "@types/commander": "^2.12.2",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "supertest": "^6.0.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@pexo/core": "0.0.0-alpha.6.1",
    "@pexo/request": "0.0.0-alpha.4.1",
    "express": "^4.17.1",
    "isomorphic-fetch": "^3.0.0",
    "react": "0.0.0-experimental-4ead6b530",
    "react-dom": "0.0.0-experimental-4ead6b530",
    "styled-components": "^5.2.1"
  },
  "alias": {
    "@pexo/core": "../core/src/core.ts",
    "@pexo/request": "../request/src/request.ts",
    "@pexo/utils": "../utils/src/utils.ts"
  }
}
