{
  "name": "@pexo/core",
  "version": "0.0.0-alpha.5.1",
  "description": "Command line interface to start a Pexo application",
  "author": "Jannick Garthen <jannick.garthen@gmail.com>",
  "license": "MIT",
  "source": "src/core.ts",
  "main": "lib/core.js",
  "browser": "lib/core.browser.js",
  "typings": "lib/core.d.ts",
  "sideEffects": false,
  "targets": {
    "main": {
